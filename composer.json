{
    "name": "genealabs/nova-horizon",
    "description": "A Laravel Nova tool.",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "laravel/framework": "^8.0|^9.0",
        "laravel/horizon": "^5.0",
        "php": ">=7.3"
    },
    "autoload": {
        "psr-4": {
            "GeneaLabs\\NovaHorizon\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "GeneaLabs\\NovaHorizon\\ToolServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
